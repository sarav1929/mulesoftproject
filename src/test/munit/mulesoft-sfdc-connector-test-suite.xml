	<munit:config name="mulesoft-sfdc-connector-test-suite.xml" />
	<munit:test name="mulesoft-sfdc-connector-test-suite-mulesoft-sfdc-accountcreationflowTest" doc:id="7116b303-af60-4fa8-9367-119c5766796e" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to mulesoft-sfdc-accountcreationflow" doc:id="025be638-335d-40cb-a9ee-ad505c6b3132" name="mulesoft-sfdc-accountcreationflow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="3566b2a6-ef8a-4c20-879e-6785d13d5292" expression="#[payload != '']" is="#[MunitTools::notNullValue()]" message="Payload is null. Plesae check your payload"/>
			<logger level="INFO" doc:name="Logger" doc:id="a32a82e3-e5a1-4c29-81a8-164475af69fa" message="Account ID is: #[payload.ID]"/>
			<choice doc:name="Choice" doc:id="bb5734db-202c-4414-86e7-9887038538ee" >
				<when expression="#[payload.ID != '']">
					<flow-ref doc:name="Flow Reference" doc:id="bd294595-d96b-4db3-93ae-feba3cbf8c38" name="mulesoft-sfdc-TSPassFlow"/>
					<set-payload value="#[payload]" doc:name="Set Payload" doc:id="f1b7a3a8-b422-4cdc-9177-106b315ec752" />
				</when>
				<when expression="#[payload.ID == '']">
					<flow-ref doc:name="Flow Reference" doc:id="0881406e-5004-45cd-af43-4ac65bc710e1" name="mulesoft-sfdc-TCFailFlow"/>
					<set-payload value="#[payload]" doc:name="Set Payload" doc:id="2118cfe3-57e4-4daa-8fcc-d5ee65460788" />
				</when>
				<otherwise>
					<logger level="INFO" doc:name="Logger" doc:id="1d1b12ae-7ba6-43e8-af0e-e34e545f244e" message="No Result"/>
				</otherwise>
			</choice>
		</munit:validation>
	</munit:test>
	<munit:test name="mulesoft-sfdc-connector-test-suite-mulesoft-sfdc-casecreationflowTest" doc:id="58d36551-3296-462e-80e0-de6e4bb615c4" description="Test">
